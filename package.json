{
  "name": "typed-css-parser",
  "version": "1.0.0",
  "description": "A detailed parser for the values of CSS properties",
  "engines": {
    "node": ">=14.17.0"
  },
  "type": "module",
  "main": "lib/index.mjs",
  "scripts": {
    "prebuild": "node ./scripts/gen-types.js",
    "build": "babel src --out-dir lib --keep-file-extension",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-flow": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-flow": "^7.22.5",
    "babel-plugin-syntax-hermes-parser": "^0.15.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-ft-flow": "^3.0.0",
    "flow-api-translator": "^0.15.1",
    "flow-bin": "^0.215.1",
    "hermes-eslint": "^0.15.1",
    "jest": "^29.5.0",
    "prettier": "3.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "snapshotFormat": {
      "printBasicPrototype": false
    },
    "testEnvironment": "node"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all"
  }
}
